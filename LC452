class Solution:
    def findMinArrowShots(self, points: List[List[int]]) -> int:
        if not points:
            return 0
        #排序關鍵點，按結束位置排序
        points.sort(key = lambda X:X[1])
        #初始化
        arrows = 1
        end = points[0][1]
        #遍歷
        for i in range(1, len(points)):
            if points[i][0] > end:
                arrows += 1
                end = points[i][1]
        return arrows
