###滑窗第二層的前哨站###(務必練熟)
from typing import List
class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        if not strs:
            return ""
        #以第一個字串做為比較基準
        prefix = strs[0]
        
        for word in strs[1:]:
            #不斷收縮prefix，直到符合當前字串開頭
            while not word.startswith(prefix):
                prefix = prefix [:-1]
                if not prefix:
                    return ""
        
        return prefix
