class Solution:
    def isValid(self, s: str) -> bool:
        stack = []
        mapping = {')':'(', ']':'[', '}':'{'}

        for char in s :
            if char in mapping: #是右括號
                top_element = stack.pop() if stack else "#"
                if mapping [char] != top_element:
                    return False
            else:               #是左括號
                stack.append(char)
        return not stack
