###Counter統計(最簡潔)
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t): #快速失敗(early return)
            return False
        #使用陣列模擬hash table(ASC2字母映射)
        count = [0]*26
        for c in s:
            count[ord(c) - ord("a")] += 1 #統計s的字符
        for c in t:
            index = ord(c) - ord("a")
            count[index] -= 1
            if count [index] < 0: #t的某字符比s多
                return False
        return True
